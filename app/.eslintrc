{
    "root": true,
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": ["plugin:vue/vue3-recommend", "eslint:recommended", "plugin:@typescript-eslint/recommend", "plugin:prettier/recommend"],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": 2020,
        "sourceType": "module",
        "jsxPragma": "React",
        "ecmaFeatures": {
            "jsx": true,
            "tsx": true
        }
    },
    "rules": {}
}
